{
  "ci": {
    "collect": {
      "url": ["http://localhost:3000/"],
      "startServerCommand": "yarn start"
    },
    "assert": {
      "assertions": {
        "unused-css-rules": ["warn"],
        "unused-javascript": ["warn"]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
