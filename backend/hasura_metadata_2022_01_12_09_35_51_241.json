{
  "resource_version": 22,
  "metadata": {
    "version": 3,
    "sources": [
      {
        "name": "main_db",
        "kind": "postgres",
        "tables": [
          {
            "table": {
              "schema": "public",
              "name": "github_pseudos"
            }
          },
          {
            "table": {
              "schema": "public",
              "name": "teams"
            },
            "array_relationships": [
              {
                "name": "team_users",
                "using": {
                  "foreign_key_constraint_on": {
                    "column": "team_name",
                    "table": {
                      "schema": "public",
                      "name": "user_team"
                    }
                  }
                }
              }
            ]
          },
          {
            "table": {
              "schema": "public",
              "name": "tools"
            }
          },
          {
            "table": {
              "schema": "public",
              "name": "user_team"
            },
            "object_relationships": [
              {
                "name": "team",
                "using": {
                  "foreign_key_constraint_on": "team_name"
                }
              },
              {
                "name": "user",
                "using": {
                  "foreign_key_constraint_on": "user_id"
                }
              }
            ]
          },
          {
            "table": {
              "schema": "public",
              "name": "users"
            },
            "array_relationships": [
              {
                "name": "user_teams",
                "using": {
                  "foreign_key_constraint_on": {
                    "column": "user_id",
                    "table": {
                      "schema": "public",
                      "name": "user_team"
                    }
                  }
                }
              }
            ]
          }
        ],
        "configuration": {
          "connection_info": {
            "use_prepared_statements": false,
            "database_url": {
              "from_env": "PG_DATABASE_URL"
            },
            "isolation_level": "read-committed"
          }
        }
      }
    ]
  }
}